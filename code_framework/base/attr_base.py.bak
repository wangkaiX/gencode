#!/usr/bin/env python3
# -*- coding: utf-8 -*-

from src.common import errno


class AttrBase:
    def __init__(self, name, errno_configs, mako_dir, errno_dir, dir, go_src_dir, gen_doc):
        self.__name = name
        self.__errno_dir = errno_dir
        self.__gen_doc = gen_doc
        self.__dir = dir
        self.__mako_dir = mako_dir

        errno_gen = errno.ErrnoGen(errno_configs)
        self.__errnos = errno_gen.errnos

    @property
    def errnos(self):
        return self.__errnos

    @property
    def name(self):
        return self.__name

    # @property
    # def errno_configs(self):
    #     return self.__errno_configs

    @property
    def errno_dir(self):
        return self.__errno_dir

    @property
    def gen_doc(self):
        return self.__gen_doc

    @property
    def dir(self):
        return self.__dir

    @property
    def mako_dir(self):
        return self.__mako_dir

